
<h1>Pirate</h1>

<div class="pirate"
  <h2>Pirate Name: <%=@pirate.name%> Weight: <%=@pirate.weight %> Height: <%=@pirate.height%> </h2>
</div> <br>

<div class = "ship">
<h2><% @ships.each do |ship|%>
<div class = "ship">
  <p>Name: <%=ship.name%>
  <p>Type:<%=ship.type%>
  <p>Booty:<%=ship.booty%>
  <%end%>
          
</div>
  
</h2>



